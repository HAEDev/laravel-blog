{
    "name": "lnch/laravel-blog",
    "description": "A complete Blog module, with category, tag, images and comments support",
    "authors": [
        {
            "name": "Tom Lynch",
            "email": "tom@lnch.co.uk"
        }
    ],
    "licence": "MIT",
    "minimum-stability": "dev",
    "require": {
        "ckeditor/ckeditor": "^4.9"
    },
    "autoload": {
        "psr-4": {
            "Lnch\\LaravelBlog\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lnch\\LaravelBlog\\LaravelBlogServiceProvider"
            ]
        }
    }
}
